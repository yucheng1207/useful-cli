{
    "name": "electron-main-app",
    "version": "1.0.0",
    "description": "A Electron application written with Typescript",
    "main": "./bld/src/main.js",
    "scripts": {
        "tsc": "rm -rf bld && ttsc",
        "tsc:watch": "ttsc -w",
        "lint": "eslint -c .eslintrc --ext .ts ./src",
        "dev:copy": "cp -r renderer/ dist && cp -r env/development.env.js src/env.js",
        "dev": "NODE_DEBUG=performance && npm run dev:copy && npm run tsc && electron ./bld/src/main.js",
        "build:mac:test": "node ./scripts/build.js --env=test --platform=mac",
        "build:win:test": "node ./scripts/build.js --env=test --platform=win",
        "build:mac:rc": "node ./scripts/build.js --env=production:rc --platform=mac",
        "build:win:rc": "node ./scripts/build.js --env=production:rc --platform=win",
        "build:mac:prod": "node ./scripts/build.js --env=production --platform=mac",
        "build:win:prod": "node ./scripts/build.js --env=production --platform=win"
    },
    "repository": "",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "typescript"
    ],
    "author": "GitHub",
    "license": "CC0-1.0",
    "devDependencies": {
        "@types/bunyan": "^1.8.7",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "chalk": "^4.1.2",
        "child_process": "^1.0.2",
        "electron": "^15.2.0",
        "electron-builder": "^22.13.1",
        "eslint": "^7.32.0",
        "minimist": "^1.2.5",
        "ttypescript": "^1.5.12",
        "typescript": "^4.4.4",
        "typescript-transform-paths": "^3.3.1"
    },
    "dependencies": {
        "bunyan": "^1.8.15"
    }
}
